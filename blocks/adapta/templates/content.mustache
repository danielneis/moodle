{{!
    @template block_adapta/content

    This template renders the content.

    Example context (json):
    {
        "id": "123456"
    }
}}
<div class="container-fluid">
  <div class="row">
    <div class="col">
      {{#exist}}
          <ul>
          {{#all}}
              <li>
              <p>{{id}}</p>
              <p>{{name}}</p>
              <p>{{value}}</p>
              </li>
          {{/all}}
          </ul>
      {{/exist}}
      {{^exist}}
        <p>{{#str}} noblocks, block_adapta {{/str}}</p>
      {{/exist}}
    </div>
  </div>
</div>
{{#js}}
require(['block_adapta/content'], function(Content) {
    Content.init('page');
});
{{/js}}
